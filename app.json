{
  "expo": {
    "name": "plantland",
    "slug": "plantland",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/leaf.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/leaf.png",
      "resizeMode": "contain",
      "backgroundColor": "#eee"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/monstera.png",
        "resizeMode": "contain",
        "backgroundColor": "#eee"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}